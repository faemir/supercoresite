{
  "entrypoint": "index.html",
  "shell": "src/supercore-app.html",
  "fragments": [
    "src/pages/supercore-home.html",
    "src/elements/supercore-game/supercore-game.html",
    "src/elements/supercore-member/supercore-member.html"
  ],
  "sources": [
    "images/**/*",
    "videos/**/*",
    "src/**/*"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/webcomponentsjs/gulpfile.js",
    "manifest.json"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
